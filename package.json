{
	"name": "ynex",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"postcss": "sass ./src/assets/scss/style.scss ./src/assets/css/style.css && postcss ./src/assets/css/style.css -o ./src/assets/css/style.css",
		"sass": "sass ./src/assets/scss/:./src/assets/css/  --load-path=node_modules",
		"sass-min": "sass ./src/assets/scss/:./src/assets/css/ --style compressed",
		"postbuild": "node postbuild.js",
		"start": "node server.cjs"
	},
	"devDependencies": {
		"@eslint/js": "^9.21.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"eslint": "^9.21.0",
		"globals": "^16.0.0",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.6",
		
		"apexcharts": "^3.54.1",
		
		"express": "^4.21.2",
		"js-cookie": "^3.0.5",
		"sass": "^1.79.5",
		"svelte-select": "^5.8.3",
		"sveltestrap": "^5.10.0",
		"svrollbar": "^0.12.0"
	}
}
